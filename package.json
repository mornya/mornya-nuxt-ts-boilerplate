{
  "name": "mornya-nuxt-ts-boilerplate",
  "version": "1.0.0",
  "description": "Nuxt.js + TypeScript development boilerplate",
  "author": "mornya78@gmail.com",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "http://github.com/mornya/mornya-nuxt-ts-boilerplate.git"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.ts --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production && npm run build && node server/index.ts",
    "generate": "nuxt generate",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx,.vue --ignore-path .gitignore .",
    "test": "jest --color --coverage --passWithNoTests --noStackTrace"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "browserslist": [
    "IE 11",
    "last 3 versions",
    "not IE < 11"
  ],
  "dependencies": {
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "nuxt": "^2.12.1"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.6.1",
    "@nuxtjs/eslint-config": "^2.0.2",
    "@nuxtjs/eslint-module": "^1.1.0",
    "@types/jest": "^25.1.4",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "@vue/composition-api": "^0.5.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.0-beta.32",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.2.4",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-nuxt": "^0.5.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^25.2.4",
    "jest-transform-stub": "^2.0.0",
    "nodemon": "^2.0.2",
    "ts-node": "^8.8.1",
    "vue-jest": "^4.0.0-0"
  }
}
